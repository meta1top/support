{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server-demo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/server-demo/tsconfig.app.json"
  },
  "monorepo": true,
  "defaultLibraryPrefix": "@meta-1",
  "root": "apps/server-demo",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json",
        "webpack": false
      }
    },
    "message": {
      "type": "library",
      "root": "libs/message",
      "entryFile": "index",
      "sourceRoot": "libs/message/src",
      "compilerOptions": {
        "tsConfigPath": "libs/message/tsconfig.lib.json",
        "webpack": false
      }
    },
    "nacos": {
      "type": "library",
      "root": "libs/nacos",
      "entryFile": "index",
      "sourceRoot": "libs/nacos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nacos/tsconfig.lib.json",
        "webpack": false
      }
    },
    "security": {
      "type": "library",
      "root": "libs/security",
      "entryFile": "index",
      "sourceRoot": "libs/security/src",
      "compilerOptions": {
        "tsConfigPath": "libs/security/tsconfig.lib.json",
        "webpack": false
      }
    },
    "server-demo": {
      "type": "application",
      "root": "apps/server-demo",
      "entryFile": "main",
      "sourceRoot": "apps/server-demo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server-demo/tsconfig.app.json"
      }
    },
    "types": {
      "type": "library",
      "root": "libs/types",
      "entryFile": "index",
      "sourceRoot": "libs/types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/types/tsconfig.lib.json",
        "webpack": false
      }
    }
  }
}